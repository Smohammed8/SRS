{% extends 'base.html.twig' %}

{% block title %}Grading{% endblock %}

{% block body %}
    <h1>Grading</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ grading.id }}</td>
            </tr>
            <tr>
                <th>Letter</th>
                <td>{{ grading.letter }}</td>
            </tr>
            <tr>
                <th>StartMark</th>
                <td>{{ grading.startMark }}</td>
            </tr>
            <tr>
                <th>EndMark</th>
                <td>{{ grading.endMark }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ grading.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_grading_index') }}">back to list</a>

    <a href="{{ path('app_grading_edit', {'id': grading.id}) }}">edit</a>

    {{ include('grading/_delete_form.html.twig') }}
{% endblock %}
